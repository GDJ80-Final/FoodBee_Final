<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gd.foodbee.mapper.RankMapper">
	<select id="selectAuthorityCodeByRankName" parameterType="String" resultType="String">
		SELECT
			authority_code
		FROM
			rank
		WHERE
			rank_name = #{rankName}
	</select>
</mapper>